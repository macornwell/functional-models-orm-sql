{
  "name": "functional-models-orm-sql",
  "version": "1.0.0",
  "description": "A sql based backend for functional-models-orm",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "watch": "tsc -w",
    "commit": "cz",
    "test": "mocha -r ts-node/register test/**/*.test.ts",
    "test:coverage": "nyc npm run test",
    "feature-tests": "node ./node_modules/.bin/cucumber-js --require ./features/stepDefinitions/*.js",
    "coverage": "nyc --all --reporter=lcov npm test",
    "build": "tsc -p ./tsconfig.build.json && cp package.json ./dist && cp README.md ./dist",
    "prettier": "prettier --write .",
    "eslint": "eslint . --fix",
    "dist": "npm run build && cd dist && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monolithst/functional-models-orm-sql.git"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "all": true,
    "include": [
      "src/**/!(*.test.*).[tj]s?(x)"
    ],
    "exclude": [
      "src/_tests_/**/*.*"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "coverage"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "orm",
    "javascript",
    "nodejs"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "author": "Mike Cornwell",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/monolithst/functional-models-orm-sql/issues"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.3.0",
    "@types/lodash": "^4.14.177",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.7",
    "@types/sinon": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.0",
    "cucumber": "^6.0.7",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-functional": "^6.0.0",
    "eslint-plugin-import": "^2.28.0",
    "mocha": "^10.4.0",
    "nyc": "^15.1.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "proxyquire": "^2.1.3",
    "sinon": "^11.1.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.4.0",
    "typescript": "^4.7.4"
  },
  "homepage": "https://github.com/monolithst/functional-models-orm-sql#readme",
  "dependencies": {
    "functional-models": "^2.1.0",
    "functional-models-orm": "^2.1.0",
    "knex": "^2.4.2",
    "lodash": "^4.17.21",
    "modern-async": "^1.1.3",
    "mysql": "^2.18.1",
    "pg": "^8.10.0",
    "sqlite3": "^5.1.6"
  }
}
